generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Competitor {
	@@map("competitor")

	id          	Int      @id @default(autoincrement())
	createdAt   	DateTime @default(now())
	updatedAt   	DateTime @updatedAt()


	active		Boolean?
	telegramId		Float?
	telegramFirstName		String?
	telegramLastName		String?
	username		String?
	password		String?
	idToken		String?
	accessToken		String?
	refreshToken		String?
	email		String?
	contextGroup		String?
	sQuizValutation		Decimal? @db.Decimal(3,2)

}

